<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:edu="http://vst/science/ontologies/networkedu.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Indicator">
    <rdfs:label xml:lang="ru">Индикатор</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Ability">
    <rdfs:label xml:lang="ru">Умение</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Descriptor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Knowledge">
    <rdfs:label xml:lang="ru">Знание</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Descriptor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Competence">
    <rdfs:label xml:lang="ru">Компетенция</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Discipline">
    <rdfs:label xml:lang="ru">Дисциплина</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Descriptor">
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Knowledge"/>
          <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Ability"/>
          <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Skill"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label xml:lang="ru">Дескриптор</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://vst/science/ontologies/networkedu.owl#Skill">
    <rdfs:label xml:lang="ru">Навык</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://vst/science/ontologies/networkedu.owl#Descriptor"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasPrevDiscipline">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasNextDiscipline"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:label xml:lang="ru">имеет_предыдущую_дисциплину</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasOutputCompetence">
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Competence"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:label xml:lang="ru">имеет_выходную_компетенцию</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasNextDiscipline">
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:label xml:lang="ru">имеет_следующую_дисциплину</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasIndicator">
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Indicator"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Competence"/>
    <rdfs:label xml:lang="ru">имеет_индикатор</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasInputCompetence">
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Competence"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Discipline"/>
    <rdfs:label xml:lang="ru">имеет_входную_компетенцию</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://vst/science/ontologies/networkedu.owl#hasDescriptor">
    <rdfs:range rdf:resource="http://vst/science/ontologies/networkedu.owl#Descriptor"/>
    <rdfs:domain rdf:resource="http://vst/science/ontologies/networkedu.owl#Indicator"/>
    <rdfs:label xml:lang="ru">имеет_дескриптор</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <edu:Discipline rdf:about="http://vst/science/ontologies/networkedu.owl#AOS">
    <edu:hasOutputCompetence>
      <edu:Competence rdf:about="http://vst/science/ontologies/networkedu.owl#CompetenceAOS">
        <edu:hasIndicator>
          <edu:Indicator rdf:about="http://vst/science/ontologies/networkedu.owl#IndicatorAOS2">
            <edu:hasDescriptor>
              <edu:Skill rdf:about="http://vst/science/ontologies/networkedu.owl#SkillAOS2">
                <rdfs:label xml:lang="ru">Разработка алгоритмического и программного продукта анализа сигналов</rdfs:label>
              </edu:Skill>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Ability rdf:about="http://vst/science/ontologies/networkedu.owl#AbilityAOS1">
                <rdfs:label xml:lang="ru">Алгоритмы цифровой обработки сигналов в системах управления</rdfs:label>
              </edu:Ability>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeAOS3">
                <rdfs:label xml:lang="ru">Представление многомерных сигналов</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <rdfs:label xml:lang="ru">ИОПК-2.2</rdfs:label>
          </edu:Indicator>
        </edu:hasIndicator>
        <edu:hasIndicator>
          <edu:Indicator rdf:about="http://vst/science/ontologies/networkedu.owl#IndicatorAOS1">
            <edu:hasDescriptor>
              <edu:Skill rdf:about="http://vst/science/ontologies/networkedu.owl#SkillAOS1">
                <rdfs:label xml:lang="ru">Навыки по цифровой обработке сигналов</rdfs:label>
              </edu:Skill>
            </edu:hasDescriptor>
            <edu:hasDescriptor rdf:resource="http://vst/science/ontologies/networkedu.owl#AbilityAOS1"/>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeAOS2">
                <rdfs:label xml:lang="ru">Системы паралельной обработки</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeAOS1">
                <rdfs:label xml:lang="ru">Методы цифровой обрабокти сигналов</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <rdfs:label xml:lang="ru">ИОПК-2.1</rdfs:label>
          </edu:Indicator>
        </edu:hasIndicator>
        <rdfs:label xml:lang="ru">ОПК-2</rdfs:label>
      </edu:Competence>
    </edu:hasOutputCompetence>
    <rdfs:label xml:lang="ru">АОС в СУ</rdfs:label>
  </edu:Discipline>
  <edu:Discipline rdf:about="http://vst/science/ontologies/networkedu.owl#OTU">
    <edu:hasOutputCompetence>
      <edu:Competence rdf:about="http://vst/science/ontologies/networkedu.owl#CompetenceOTU">
        <edu:hasIndicator>
          <edu:Indicator rdf:about="http://vst/science/ontologies/networkedu.owl#IndicatorOTU">
            <rdfs:label xml:lang="ru">ИПКС-1.1</rdfs:label>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeOTU3">
                <rdfs:label xml:lang="ru">Звенья и схемы автоматических систем</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Skill rdf:about="http://vst/science/ontologies/networkedu.owl#SkillOTU1">
                <rdfs:label xml:lang="ru">Методы моделирования процессов управления</rdfs:label>
              </edu:Skill>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Ability rdf:about="http://vst/science/ontologies/networkedu.owl#AbilityOTU4">
                <rdfs:label xml:lang="ru">Технические средства для систем регулирования</rdfs:label>
              </edu:Ability>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Ability rdf:about="http://vst/science/ontologies/networkedu.owl#AbilityOTU3">
                <rdfs:label xml:lang="ru">Составление математичсеких моделей объектов</rdfs:label>
              </edu:Ability>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeOTU2">
                <rdfs:label xml:lang="ru">Принципы автоматического управления</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Ability rdf:about="http://vst/science/ontologies/networkedu.owl#AbilityOTU2">
                <rdfs:label xml:lang="ru">Проектирование систем автоматического управления</rdfs:label>
              </edu:Ability>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Skill rdf:about="http://vst/science/ontologies/networkedu.owl#SkillOTU3">
                <rdfs:label xml:lang="ru">Стандарты в области автоматизации и метрологии</rdfs:label>
              </edu:Skill>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeOTU1">
                <rdfs:label xml:lang="ru">Основы моделирования и расчетов в области автоматики</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeOTU5">
                <rdfs:label xml:lang="ru">Принципы управления техническими системами</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Ability rdf:about="http://vst/science/ontologies/networkedu.owl#AbilityOTU1">
                <rdfs:label xml:lang="ru">Компьютерное моделирование систем управления</rdfs:label>
              </edu:Ability>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Skill rdf:about="http://vst/science/ontologies/networkedu.owl#SkillOTU2">
                <rdfs:label xml:lang="ru">Навыки работы с основными измерительными приборами</rdfs:label>
              </edu:Skill>
            </edu:hasDescriptor>
            <edu:hasDescriptor>
              <edu:Knowledge rdf:about="http://vst/science/ontologies/networkedu.owl#KnowledgeOTU4">
                <rdfs:label xml:lang="ru">Методы анализа устойчивости систем</rdfs:label>
              </edu:Knowledge>
            </edu:hasDescriptor>
          </edu:Indicator>
        </edu:hasIndicator>
        <rdfs:label xml:lang="ru">ПКС-1</rdfs:label>
      </edu:Competence>
    </edu:hasOutputCompetence>
    <edu:hasNextDiscipline rdf:resource="http://vst/science/ontologies/networkedu.owl#OTU"/>
    <rdfs:label xml:lang="ru">ОТУ</rdfs:label>
  </edu:Discipline>
</rdf:RDF>
